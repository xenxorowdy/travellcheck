{
  "expo": {
    "name": "the heritage tour",
    "slug": "theheritagetour",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo-tht.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Logo-tht.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xenxtravel.travellcheck"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo-tht.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.xenxtravel.travellcheck"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc70c30f-cc38-4c21-8e54-80ab1dedb252"
      }
    }
  }
}